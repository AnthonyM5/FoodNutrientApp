<!DOCTYPE html>
<html>
  <head>
    <title>FoodNutrientApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

<body data-turbolinks="false">
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="<%= root_path %>">FoodNutrientApp</a>
  
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav">
      <a class="nav-item nav-link active" href="<%= foods_path %>">Food Index <span class="sr-only">(current)</span></a>
       <a class="nav-item nav-link active" href="<%= public_path %>">Public Meals<span class="sr-only">(current)</span></a>
      <% if current_user %>
      <a class="nav-item nav-link active" href="<%= meals_path %>">My Meals<span class="sr-only">(current)</span></a>
      <a class="nav-item nav-link" href="<%= destroy_user_session_path %>">Log Out</a>
      <% else %>
      <a class="nav-item nav-link" active" href="<%= new_user_session_path %>">Login<span class="sr-only">(current)</span></a>
      <a class="nav-item nav-link" active" href="<%= new_user_registration_path	%>">Sign Up<span class="sr-only">(current)</span></a>
      <% end %>
    </div>
  </div>
</nav>
  <% if flash[:errors] %>
  <div class="alert alert-primary" role="alert"><%= flash[:errors]%></div>
  <% end %>
          <%= yield %>
     <p class="notice"><%= notice %></p>
     <p class="alert"><%= alert %></p>
    
  </body>
</html>




