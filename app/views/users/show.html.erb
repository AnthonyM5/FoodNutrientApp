
<h1>Welcome! <%= current_user.name %></h1>
<h3> User ID: <%= current_user.id %></h3>
<h3>All My Foods</h3>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Food ID#:</th>
      <th scope="col">Food Name:</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    <th scope="row"></th>
    <% current_user.foods.uniq.each do |food| %>
    <tr>
      <th scope="row"><%= food.id %></th>
      <td><%= link_to "#{food.name}", food_path(food.id) %></td>
      <% end %>
    </tr>
  </tbody>
</table>


